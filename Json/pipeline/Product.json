				"typeProperties": {
					"source": {
						"type": "MySqlSource",
						"query": "SELECT\np1.name AS lev1, \np2.name AS Manufacturer, \np3.name AS Brand,\np4.name AS SubBrand,\np5.name AS RebateBrand,\np3.id AS BrandID,\np4.id AS SubBrandID, \np5.id AS RebateBrandID,\n\nCASE \nWHEN p5.id IS NULL AND p4.id IS NOT NULL THEN p4.id \nWHEN p4.id IS NULL AND p5.id IS NULL THEN p3.id \n\nELSE p5.id END AS MasterKey\n\nFROM producttree AS p1\nLEFT JOIN producttree AS p2 ON p2.parentid=p1.id\nLEFT JOIN producttree AS p3 ON p3.parentid=p2.id\nLEFT JOIN producttree AS p4 ON p4.parentid=p3.id\nLEFT JOIN producttree AS p5 ON p5.parentid=p4.id\n\nWHERE p1.name='Product';\n"
					},
